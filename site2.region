<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>    
    <link href="https://fonts.googleapis.com/css?family=PT+Sans|Raleway" rel="stylesheet">
    <!-- include system scripts, page meta, and header code injection -->
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">

    <div class="site-container">

      <header class="site-header">
        <!-- site navigation -->
        <squarespace:navigation navigationId="siteNav" template="site-navigation" />

        <!-- site title or logo -->
        <!--{.section website}
          <h1 class="site-title-heading" data-content-field="site-title">
            <a href="/" class="site-title-link">{siteTitle}</a>
          </h1>
          {.end}-->
      </header>

      <!-- cms content injection point -->
      <main class="content-container" role="main" data-content-field="main-content">
        {squarespace.main-content}
        <div class="container-fluid">
          <div class="row no-gutters">
            <div class="col-md-12 col-sm-12 carousel">
              <div class="img-wrap">  
                <img src="https://i.imgur.com/EGRKwwy.jpg"></img>
                <div class="hilight">
                  <p>Big Mac mmmmm</p>
                </div>
              </div>  
            </div>
          <div class="row no-gutters equal">
            <div class="col-md-4 col-sm-12 advert">
              <div class="img-wrap">
                <img src="https://i.imgur.com/zfkB4fc.jpg"></img>
                <div class="hilight">
                  <p>Get Some Fries</p>
                </div>
              </div>  
            </div> 
            <div class="col-md-4 col-sm-12 feature">
              <div class="img-wrap">
                <img src="https://i.imgur.com/v87eRxG.jpg"></img>
                <div class="hilight">
                  <p>That's a big ass check</p>
                </div>
              </div>  
            </div>  
            <div class="col-md-4 col-sm-12 feature">
              <div class="img-wrap">
                <img src="https://i.imgur.com/dkxxX8o.jpg"></img>
                <div class="hilight">
                  <p>Ronald Pope</p>
                </div>
              </div>  
            </div>  
          </div>
          <div class="row no-gutters">
            <div class="col-md-8 col-sm-12 twitter">
              <blockquote class="twitter-tweet" data-lang="en"><p lang="en" dir="ltr">Having a hot study buddy never hurt anyone. Enjoy <a href="https://twitter.com/hashtag/BackInSchool?src=hash&amp;ref_src=twsrc%5Etfw">#BackInSchool</a> with <a href="https://twitter.com/hashtag/McCafe?src=hash&amp;ref_src=twsrc%5Etfw">#McCafe</a> â˜• <a href="https://t.co/XGy8zXoIbY">pic.twitter.com/XGy8zXoIbY</a></p>&mdash; McDonald&#39;s (@McDonalds) <a href="https://twitter.com/McDonalds/status/915637656280797184?ref_src=twsrc%5Etfw">October 4, 2017</a></blockquote>
              <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
            </div> 
            <div class="col-md-4 col-sm-12 advert">
              <div class="img-wrap">
                <img src="https://i.imgur.com/ZFTYzzY.jpg"></img>
                <h2>Ad2</h2>
              </div>
            </div>  
        </div> 
      </main>
      <!--Footer with open block field -->
      <footer class="site-footer">
        <squarespace:block-field id="footerBlocks" columns="12" />
      </footer>

    </div>

    <!-- combo and minify scripts when not logged in -->
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />

    <!-- other scripts, and footer code injection -->
    {squarespace-footers}

  </body>
</html>
